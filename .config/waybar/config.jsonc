        "format-icons": {                                                                                                             
            "headphone": "ÔÄ•",                                                                                                         
            "hands-free": "Ôñê",                                                                                                        
            "headset": "Ôñê",                                                                                                           
            "phone": "ÔÇï",                                                                                                             
            "portable": "ÔÇï",                                                                                                          
            "car": "ÔÜπ",                                                                                                          
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]                                                                                                },
		"on-click": "pavucontrol"},
	
	"network": {
	    "format-wifi": "{signalStrength}% Ôá´ ‚Üì{bandwidthDownBits} ‚Üë{bandwidthUpBits}",
	    "format-ethernet": "Óºâ ‚Üì{bandwidthDownBits} ‚Üë{bandwidthUpBits}",
	    "format-disconnected": "Ó¨Å",
	    "tooltip-format-disconnected": "Error",
	    "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´\n‚Üì{bandwidthDownBits} ‚Üë{bandwidthUpBits}",
	    "tooltip-format-ethernet": "{ifname} üñß\n‚Üì{bandwidthDownBits} ‚Üë{bandwidthUpBits}",
	    "on-click": "kitty nmtui"
	},

        "bluetooth": {
            "format-on": "Û∞ÇØ {status}",
            "format-off": "BT-off",
            "format-disabled": "Û∞Ç≤",
            "format-connected-battery": "{device_battery_percentage}% Û∞ÇØ",
            "states": {
    		    "off": "off",
        	    "disabled": "disabled"
	    },
            "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
            "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
            "tooltip-format-enumerate-connected": "{device_alias}\n{device_address}",
            "tooltip-format-enumerate-connected-battery": "{device_alias}\n{device_address}\n{device_battery_percentage}%",
            "on-click": "blueman-manager"
        }, 
        "battery": {
            "interval":1,
            "states": {
                "good": 95,
                "warning": 50,
                "critical": 20
            },
            "format": "{capacity}% {icon}",
            "format-charging": "{capacity}% Û∞ÇÑ",
            "format-plugged": "{capacity}% Û∞ÇÑ Ó¨≠",
            "format-alt": "{time} {icon}",
            "format-icons": [
                "Û∞Åª",
            "Û∞Åº",
            "Û∞Åæ",
            "Û∞ÇÄ",
            "Û∞ÇÇ",
            "Û∞Åπ"
            ]
        },
        "cpu": {
            "format": "CPU : {usage}% Ôãõ ",
	    "interval": 1,
            "tooltip": true,
            "states": {
		    "normal": 30,
        	    "warning": 60,
        	    "critical": 90
	    }
        },
        "memory": {
            "format": "RAM : {}% \uf1c0",
	    "interval": 1,
	    "states": {
                    "normal": 30,
                    "warning": 50,
                    "critical": 70
            }
        },

	"custom/weather": {                                                                                                          
            "format": "{text}",                                                                                                          
            "tooltip": false,                                                                                                            
            "interval": 1800,                                                                                                               
            "exec": "$HOME/.config/waybar/scripts/weather.py",                                                                                                   
            "return-type": "json"                                                                                                        
        }, 


	"custom/location": {
	    "format": "{text}",
	    "tooltip": false,
	    "interval": 1800,
	    "exec": "$HOME/.config/waybar/scripts/location.py",
	    "return-type": "json"
	},

	"custom/temperature": {
            "format": "{text}",
            "tooltip": false,
            "interval": 1800,
            "exec": "$HOME/.config/waybar/scripts/temp.py",
            "return-type": "json"
        },

	"custom/humidity": {
            "format": "{text}",
            "tooltip": false,
            "interval": 1800,
            "exec": "$HOME/.config/waybar/scripts/humidity.py",
            "return-type": "json"
        },

	"custom/precipitation": {
            "format": "{text}",
            "interval": 1800,
            "exec": "$HOME/.config/waybar/scripts/precipitation.py",
            "return-type": "json"
        },

"backlight": { 
