{
        "layer": "top",
        "position": "top",
        "reload_style_on_change": true,
        "modules-left": ["custom/weather","custom/location","custom/temperature","custom/humidity","custom/precipitation","hyprland/workspaces","hyprland/window"], 
        "modules-center": ["pulseaudio", "bluetooth", "backlight"],
        "modules-right": ["memory","cpu","network","battery","clock"],


        "hyprland/workspaces": {
            "format": "{icon}",
            "format-icons": {
                "default": "ÔÑë",                                                                                           
	         "active": "ÔÜí"   
            }
        },

	"hyprland/window": {
		"format": "<span font='9' rise='-4444'>{}</span>"
	},

        "clock": {
            "format": "{:%I:%M:%S %p} ",
            "interval": 1,   
            "tooltip-format": "<tt>{calendar}</tt>",
            "calendar": {
                "format": {
                    "today": "<span color='#ff0000'><b>{}</b></span>"
                }
            }
        },

	"pulseaudio": {                                                                                                                   
